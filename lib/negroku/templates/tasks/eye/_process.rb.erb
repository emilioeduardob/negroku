  process '<%= process %>' do
    pid_file '<%= options[:pid_file]%>'
    start_command '/home/deploy/.rbenv/bin/rbenv exec <%= options[:start_command]%>'
    <% if options[:stop_command] %>
    stop_command '<%= options[:stop_command] %>'
    <% else %>
    stop_signals [:QUIT, 2.seconds, :KILL]
    <% end %>
  <% options[:check].each do |check, values| -%>
  <%= "check :#{check}, #{values}" %>
  <% end -%>

  end
